.container {
  margin-bottom: 150rpx;
  margin-top: 150rpx;
}
.header-container {
  background-color: #fff;
}

.iphone {
  margin-top: 200rpx;
}
.goods-box {
  width: 90%;
  margin: 0 auto;
  margin-top: 20rpx;
  height: 160rpx;
  margin-bottom: 20rpx;
}

.goods-box image {
  width: 160rpx;
  height: 160rpx;
  float: left;
  border-radius: 4rpx;
}

.goods-box .goods-info {
  float: left;
  margin-left: 20rpx;
  width: 70%;
  height: 160rpx;
}

.goods-box .goods-title {
  font-size: 32rpx;
  color: #333;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.goods-box .goods-text {
  font-size: 28rpx;
  color: #999;
  margin-top: 10rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.goods-box .goods-tuan {
  margin-top: 10rpx;
  font-size: 28rpx;
  color: #999;
}

.goods-box .goods-tuan text {
  color: #b5272d;
  font-size: 38rpx;
  margin-left: 10rpx;
}

.pingtuan-box {
  border-top: 20rpx solid #f5f5f5;
  width: 100%;
}

.pingtuan-info-box {
  width: 80%;
  margin: 0 auto;
  height: 200rpx;
  margin-top: 60rpx;
  position: relative;
}

.pingtuan-info-box .line {
  position: absolute;
  width: 400rpx;
  height: 1rpx;
  background-color: #b5272d;
  top: 66rpx;
  right: 100rpx;
}

.pingtuan-info {
  width: 50%;
  height: 200rpx;
  text-align: center;
  float: left;
  position: relative;
}

.pingtuan-info text {
  position: absolute;
  width: 90rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 26rpx;
  background-color: #b5272d;
  color: #fff;
  border-radius: 50rpx;
  top: 0;
  right: 20rpx;
}

.pingtuan-info .pingtuan-info-name {
  font-size: 28rpx;
  margin-top: 20rpx;
  color: #999;
}

.pingtuan-info image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  border: 1px solid #b5272d;
  display: block;
  margin: 0 auto;
  background-color: #fff;
}

.pingtuan-info-number {
  margin-top: 30rpx;
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  color: #333;
}

.pingtuan-info-number text {
  color: #b5272d;
  font-size: 36rpx;
  margin-left: 8rpx;
  margin-right: 8rpx;
}

.pingtuan-info-times {
  text-align: center;
  margin-top: 50rpx;
  width: 100%;
  font-size: 30rpx;
  height: 60rpx;
  color: #999;
  position: relative;
}

.pingtuan-info-times .lin-l {
  position: absolute;
  width: 120rpx;
  height: 1rpx;
  background-color: #ddd;
  left: 0;
  top: 10px;
}

.pingtuan-info-times .lin-r {
  position: absolute;
  width: 120rpx;
  height: 1rpx;
  background-color: #ddd;
  right: 0;
  top: 10px;
}

.pingtuan-info-times text {
  margin-right: 10rpx;
}

.pingtuan-info-times .day, .pingtuan-info-times .hou, .pingtuan-info-times .min,
.pingtuan-info-times .con {
  background-color: #636363;
  padding: 6rpx 10rpx;
  color: #fff;
  font-size: 30rpx;
  border-radius: 4rpx;
  margin-right: 10rpx;
}

.footer {
  position: fixed;
  width: 100%;
  height: 100rpx;
  bottom: 0;
  box-shadow: 0 3px 5px #b5272d;
  background-color: #fff;
}

.footer .home {
  float: left;
  width: 25%;
  height: 100rpx;
  text-align: center;
  position: relative;
}

.footer .home button {
  width: 100%;
  height: 100rpx;
  position: absolute;
  top: 0;
  left: 0;
  opacity: 0;
}

.footer .home image {
  width: 42rpx;
  height: 42rpx;
  display: block;
  margin: 0 auto;
  margin-top: 10rpx;
}

.footer .home text {
  display: block;
  margin-top: 2rpx;
  font-size: 26rpx;
  color: #b5272d;
}

.footer .goPingtuan {
  float: left;
  height: 100rpx;
  background-color: #b5272d;
  width: 75%;
  text-align: center;
  color: #fff;
  line-height: 100rpx;
  font-size: 32rpx;
  position: relative;
}

.footer .goPingtuan button {
  width: 100%;
  height: 100rpx;
  position: absolute;
  top: 0;
  left: 0;
  opacity: 0;
}

.ptuan-joiner-box {
  width: 100%;
  margin-top: 30rpx;
}

.ptuan-joiner-list {
  width: 90%;
  margin: 0 auto;
  height: 100rpx;
  margin-bottom: 20rpx;
  padding-bottom: 20rpx;
  border-bottom: 1px dashed #f5f5f5;
}

.ptuan-joiner-list image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  float: left;
}

.ptuan-joiner-list .joiner-name {
  float: left;
  width: 50%;
  font-size: 30rpx;
  margin-top: 12rpx;
  margin-left: 12rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.ptuan-joiner-list .joiner-time {
  float: left;
  width: 50%;
  font-size: 24rpx;
  color: #999;
  margin-top: 4rpx;
  margin-left: 12rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.ptuan-joiner-list .btn {
  float: right;
  color: #fff;
  background-color: #b5272d;
  font-size: 26rpx;
  width: 80rpx;
  height: 46rpx;
  line-height: 23px;
  text-align: center;
  border-radius: 4rpx;
  margin-top: -26rpx;
}

.ptuan-joiner-list .l {
  background-color: #999;
}

.show-popup {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 9999999;
}

.popup-mask {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 5;
}

.popup-contents {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  background-color: #fff;
  z-index: 6;
}

.pop-goods-info {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-left: 30rpx;
  padding: 30rpx 0;
  border-bottom: 1px solid #f5f5f5;
}

.pop-img-box {
  width: 120rpx;
  height: 120rpx;
  overflow: hidden;
  margin-right: 26rpx;
}

.pop-img-box .goods-thumbnail {
  width: 120rpx;
  height: 120rpx;
}

.pop-goods-title {
  width: 484rpx;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  font-size: 26rpx;
  color: #000;
}

.pop-goods-price {
  font-size: 26rpx;
  color: #b5272d;
  margin-top: 20rpx;
}

.pop-goods-close {
  width: 36rpx;
  height: 36rpx;
  flex: 1;
  background: url("https://cdn.it120.cc/images/weappshop/popup-close.png") no-repeat center center;
  background-size: 36rpx auto;
  align-self: flex-start;
  margin-top: -10rpx;
}

.size-label-box  .label {
  font-size: 26rpx;
  color: #000;
  padding-left: 30rpx;
  padding: 30rpx 0 20rpx 30rpx;
}

.size-label-box .label-item-box {
  display: flex;
  margin-left: 30rpx;
  flex-direction: row;
  flex-wrap: wrap;
}

.size-label-box .label-item {
  font-size: 26rpx;
  color: #000;
  padding: 14rpx 20rpx;
  border: 1px solid #ddd;
  border-radius: 6rpx;
  margin: 0 20rpx 20rpx 0;
}

.size-label-box .label-item.active {
  color: #b5272d;
  border: 1px solid #b5272d;
}

.buy-num-box {
  display: flex;
  justify-content: space-between;
  padding: 30rpx 30rpx 48rpx 0;
  margin-left: 30rpx;
  border-top: 1px solid #f5f5f5;
  margin-top: 30rpx;
  align-items: center;
}

.num-label {
  font-size: 26rpx;
  color: #000;
}

.buy-num-box .num-box {
  display: flex;
}

.buy-num-box .num-box .num-jian, .buy-num-box .num-box .num-input,
.buy-num-box .num-box .num-jia {
  width: 80rpx;
  height: 64rpx;
  line-height: 62rpx;
  text-align: center;
  border: 1px solid #eee;
}

.buy-num-box .num-box .num-input {
  font-size: 28rpx;
}

.buy-num-box .num-box .num-input input {
  height: 100%;
}

.popup-join-btn {
  width: 100%;
  height: 89rpx;
  text-align: center;
  line-height: 89rpx;
  font-size: 34rpx;
  color: #fff;
  background-color: #b5272d;
}

.buy-num-box .num-box .hui {
  background-color: #f5f5f9;
}

.vitery-ptuan-box {
  width: 100%;
}

.vitery-ptuan {
  text-align: center;
  margin-top: 100px;
  color: #999;
  font-size: 32rpx;
}

.vitery-ptuan-btn {
  background-color: #b5272d;
  width: 280rpx;
  margin: 0 auto;
  font-size: 32rpx;
  text-align: center;
  height: 70rpx;
  line-height: 70rpx;
  color: #fff;
  border-radius: 10rpx;
  margin-top: 30rpx;
}

/*----------show----------*/

.show-container {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: 99999999999;
}

.show-container .show-box {
  width: 70%;
  margin: 0 auto;
  height: 600rpx;
  background-color: #fff;
  margin-top: 40%;
  border-radius: 10rpx;
}

.show-logo-box {
  text-align: center;
  width: 80%;
  border-bottom: 1px solid #f5f5f5;
  margin: 0 auto;
  padding-top: 20px;
}

.show-logo-box image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  display: block;
  margin: 0 auto;
}

.show-mall-name {
  font-size: 32rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  color: #333;
}

.show-text-box {
  margin-top: 20rpx;
  margin-bottom: 40rpx;
}

.show-text-name {
  font-size: 30rpx;
  color: #333;
  width: 80%;
  margin: 0 auto;
}

.show-text-info {
  width: 80%;
  margin: 0 auto;
  font-size: 24rpx;
  color: #999;
  margin-top: 10rpx;
}

.show-text-info text {
  margin-right: 10rpx;
  font-size: 26rpx;
}

.show-box button::after {
  border-radius: 4rpx;
  border: 0;
}

.show-box button {
  width: 80%;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 30rpx;
  background-color: #04be01;
  color: #fff;
}

.show-share {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  background-color: rgba(0, 0, 0, 0.4);
  z-index: 999999;
}

.show-close-share {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 999999;
}

.show-share-box {
  width: 70%;
  background-color: #fff;
  height: 400rpx;
  overflow: hidden;
  border-radius: 10rpx;
  z-index: 9999999;
  position: fixed;
  left: 0;
  bottom: 0;
  top: 35%;
  right: 0;
  margin: 0 auto;
}

.show-share-title {
  text-align: center;
  color: #b5272d;
  font-size: 34rpx;
  margin-top: 10%;
}

.show-share-btn-box {
  width: 80%;
  margin: 0 auto;
  margin-top: 15%;
}

.show-share-share {
  display: inline-block;
  width: 50%;
  height: 160rpx;
  position: relative;
}

.show-share-share image {
  width: 46rpx;
  height: 46rpx;
  display: block;
  margin: 0 auto;
  background-color: #b5272d;
  padding: 26rpx;
  border-radius: 50%;
}

.show-share-share text {
  font-size: 28rpx;
  display: block;
  text-align: center;
  color: #b5272d;
  margin-top: 6rpx;
}

.show-share-share .pop-box-btn {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 160rpx;
  opacity: 0;
}

.show-share-code {
  display: inline-block;
  width: 50%;
  height: 160rpx;
  position: relative;
}

.show-share-code image {
  width: 50rpx;
  height: 50rpx;
  display: block;
  margin: 0 auto;
  background-color: #b5272d;
  padding: 24rpx;
  border-radius: 50%;
}

.show-share-code text {
  font-size: 28rpx;
  display: block;
  text-align: center;
  color: #b5272d;
  margin-top: 6rpx;
}

.show-code-box {
  width: 70%;
  background-color: #fff;
  height: 610rpx;
  overflow: hidden;
  border-radius: 10rpx;
  z-index: 9999999;
  position: fixed;
  left: 0;
  bottom: 0;
  top: 25%;
  right: 0;
  margin: 0 auto;
}
.show-code-re{
  width: 100%;
  height: 610rpx;
  position: relative;
}
.show-code-box image {
  width: 480rpx;
  height: 480rpx;
  display: block;
  margin: 0 auto;
  padding-top: 12rpx;
}

.show-code-box .show-code-btn {
  position: absolute;
  bottom: 0;
  width: 100%;
  text-align: center;
  background-color: #b5272d;
  color: #fff;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
}
